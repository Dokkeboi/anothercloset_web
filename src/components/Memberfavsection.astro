---
//${}
import Productcard from "./Productcard.astro";
import Infocard from "./Infocard.astro";
---

<!-- Herunder skal selvfølgelig defineres dynamisk, når det kommer til stykket: -->
<h1>Medlemmers favoritter</h1>
<section>
  <Productcard />

  <!-- <Infocard
    cardtitle="Og meget mere..."
    cardtext="Tilmeld dig Another Closet og begynd at låne i dag!"
    cardcta="Bliv medlem"
    cardlink="#"
  /> -->
</section>

<style>
  section {
    margin-left: 33px;
    margin-right: 33px;
    margin-bottom: 233px;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 0.5%;
  }
  @media (min-width: 800px) {
    section {
      margin-left: 145px;
      margin-right: 145px;
      display: grid;
      grid-template-columns: 1fr 1fr 1fr;
    }
  }
</style>
